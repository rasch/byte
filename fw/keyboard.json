{
  "keyboard_name": "byte",
  "maintainer": "rasch",
  "manufacturer": "rAsCH.co",
  "url": "https://github.com/rasch/byte",
  "bootloader_instructions": "press the reset button",
  "tags": ["ortho", "one-handed", "card"],
  "bootloader": "stm32-dfu",
  "processor": "STM32F072",
  "build": {
    "firmware_format": "bin",
    "lto": true
  },
  "features": {
    "caps_word": true,
    "combo": true,
    "command": false,
    "console": false,
    "extrakey": true,
    "mousekey": true,
    "nkro": true
  },
  "tapping": {
    "permissive_hold": true
  },
  "caps_word": {
    "double_tap_shift_turns_on": true
  },
  "indicators": {
    "caps_lock": "A8"
  },
  "layouts": {
    "LAYOUT": {
      "layout": [
        { "matrix": [0, 0], "x": 0, "y": 0 },
        { "matrix": [0, 1], "x": 1, "y": 0 },
        { "matrix": [0, 2], "x": 2, "y": 0 },
        { "matrix": [0, 3], "x": 3, "y": 0 },
        { "matrix": [1, 0], "x": 0, "y": 1 },
        { "matrix": [1, 1], "x": 1, "y": 1 },
        { "matrix": [1, 2], "x": 2, "y": 1 },
        { "matrix": [1, 3], "x": 3, "y": 1 }
      ]
    }
  },
  "matrix_pins": {
    "direct": [
      ["B3", "B4", "B15", "B1"],
      ["B5", "B14", "B0", "B2"]
    ]
  },
  "mouse_key": {
    "delay": 100,
    "interval": 16,
    "time_to_max": 40,
    "wheel_delay": 100
  },
  "usb": {
    "device_version": "0.1.0",
    "pid": "0x0008",
    "vid": "0xDAB5"
  }
}
